<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/unnamed"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:contentDescription="@drawable/ertertert"
        android:scaleType="centerCrop"
        android:src="@drawable/ertertert" />

    <RadioGroup
        android:id="@+id/radioGroup1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true" >
    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroup2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/radioGroup1" >
    </RadioGroup>

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView1"
        android:layout_alignRight="@+id/imageView1"
        android:background="#00000000"
        android:text="@string/v10" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_below="@+id/radioGroup2"
        android:layout_marginTop="36dp"
        android:src="@drawable/icon1" />

    <Button
        android:id="@+id/start_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_below="@+id/pro_Bar"
        android:layout_marginTop="13dp"
        android:text="@string/hack"
        /> 
     
       />

    <requestFocus />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView2"
        android:layout_below="@+id/imageView2"
        android:maxLength="10"
        android:src="@drawable/icon2" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView3"
        android:layout_below="@+id/imageView3"
        android:src="@drawable/icon3" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_below="@+id/imageView4"
        android:maxLength="10"
        android:src="@drawable/icon4" />

    <Button
        android:id="@+id/soBT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_alignRight="@+id/imageView1"
        android:layout_below="@+id/start_button"
        android:autoLink="web"
        android:clickable="true"
        android:cursorVisible="true"
        android:enabled="false"
        android:linksClickable="true"
        android:text="@string/Update" />

    <EditText
        android:id="@+id/ET0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView3"
        android:layout_alignTop="@+id/imageView2"
        android:layout_toRightOf="@+id/imageView2"
        android:background="@android:drawable/edit_text"
        android:ems="10"
        android:hint="value"
        android:inputType="number"
        android:maxLength="10"
        android:padding="5dp"
        android:textColor="@android:color/primary_text_light"
        android:textSize="@dimen/font_size" />

    <EditText
        android:id="@+id/ET1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView4"
        android:layout_below="@+id/imageView2"
        android:layout_toRightOf="@+id/imageView2"
        android:background="@android:drawable/edit_text"
        android:ems="10"
        android:hint="value"
        android:inputType="number"
        android:maxLength="10"
        android:padding="6dp"
        android:textColor="@android:color/primary_text_light"
        android:textSize="@dimen/font_size" />

    <EditText
        android:id="@+id/ET2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView5"
        android:layout_alignTop="@+id/imageView4"
        android:layout_toRightOf="@+id/imageView4"
        android:background="@android:drawable/edit_text"
        android:ems="10"
        android:hint="value"
        android:inputType="number"
        android:maxLength="10"
        android:padding="5dp"
        android:textColor="@android:color/primary_text_light"
        android:textSize="@dimen/font_size" />

    <ProgressBar
        android:id="@+id/pro_Bar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/imageView5"
        android:layout_marginTop="19dp"
        android:minWidth="710dp" />

    <EditText
        android:id="@+id/ET3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView5"
        android:layout_alignTop="@+id/imageView5"
        android:layout_toRightOf="@+id/imageView5"
        android:background="@android:drawable/edit_text"
        android:ems="10"
        android:hint="value"
        android:inputType="number"
        android:maxLength="10"
        android:padding="5dp"
        android:textColor="@android:color/primary_text_light"
        android:textSize="@dimen/font_size" />

    <Button
        android:id="@+id/suBT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_alignRight="@+id/imageView1"
        android:layout_below="@+id/soBT"
        android:autoLink="web"
        android:clickable="true"
        android:text="About" />

</RelativeLayout>